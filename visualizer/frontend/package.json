{
  "name": "maap-visualizer",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://yassineayao.github.io/maap-visualizer",
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "leaflet": "^1.8.0",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-date-range": "^1.4.0",
    "react-dom": "^18.0.0",
    "react-leaflet": "^4.0.0",
    "react-leaflet-vectorgrid": "^2.2.1",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4",
    "webpack-cli": "^4.10.0"
  },
  "scripts": {
    "start": "node --max_old_space_size=2560 node_modules/.bin/react-scripts start",
    "dev": "webpack --mode development --entry ./src/index.js --output-path ./output && cp -r ./output/* ../backend/static/main/",
    "build": "webpack --mode production --entry ./src/index.js --output-path ./output && cp -r ./output/* ../backend/static/main/",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "gh-pages": "^3.2.3"
  }
}
